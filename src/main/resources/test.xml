<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="son" class="practice.apollo.Son">
        <constructor-arg value="ddd"/>
    </bean>

    <bean id="person" class="practice.apollo.PersonBean">
        <constructor-arg value="madepeng"/>
        <constructor-arg value="22"/>
        <constructor-arg>
            <list>
                <value>2</value>
                <value>3</value>
                <value>4</value>
            </list>
        </constructor-arg>
        <constructor-arg ref="son"/>
    </bean>

    <bean id="deparment" class="practice.apollo.Department">
        <property name="code" value="222"/>
        <property name="name" value="shengtaijinrong"/>
    </bean>
</beans>